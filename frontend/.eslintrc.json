{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "ignorePatterns": ["src/Testing/**/*.js"],
  "rules": {
    "react/prop-types": 0
  }
}
